
# display histgram of angle(pitch and yaw) in 2 plots
import matplotlib.pyplot as plt
#plt.figure()#clear cache
plt.subplot(1, 2, 1)
plt.hist(angles[:, 0], bins = 18, range = (-90, 90))
plt.title('pitch')
plt.subplot(1, 2, 2)
plt.hist(angels[:, 1], bins = 18, range = (-90, 90)
plt.title('yaw')
plt.show()

